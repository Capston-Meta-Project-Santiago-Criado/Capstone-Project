generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


  model Company {
    id             Int    @id @default(autoincrement())
    name           String
    ticker         String
    tenk_url       String
    fivek_urls     String 
    earnings_calls String 
    daily_price    Int 
    recent_news    String[] 
    created_at DateTime @default(now())
  }

  model User { 
    id              Int    @id @default(autoincrement())
    username        String  @unique
    name            String
    email           String  @unique 
    password        Int      
    search_history  String[] 
  }